<model name="deliveryDto" activable="false" auditable="false" deletable="false" insertable="false"
       updatable="false"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="http://schema.qcadoo.org/model"
       xsi:schemaLocation="http://schema.qcadoo.org/model http://schema.qcadoo.org/model.xsd">

    <fields>
        <string name="number"/>
        <string name="name"/>
        <string name="supplierName"/>
        <string name="contractorCategory"/>
        <datetime name="deliveryDate"/>
        <enum name="state"
              values="01draft,02prepared,03duringCorrection,04declined,05approved,06received,07accepted"/>
        <datetime name="createDate"/>
        <string name="locationNumber"/>
        <integer name="locationId"/>
        <datetime name="dateOfSendingEmail"/>
        <boolean name="releasedForPayment"/>
        <string name="paymentID"/>
        <boolean name="paid"/>
        <date name="paymentDate"/>
        <integer name="repaymentTime"/>
        <string name="relatedDeliveryNumber"/>
        <string name="createUser"/>
        <string name="paidCellColor"/>
        <boolean name="wms"/>
        <enum name="stateInWMS" values="01waiting,02inProgress,03delayed,04realized"/>
    </fields>

    <masterModel plugin="deliveries" model="delivery"/>
</model>
