<windowTabExtension xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xmlns="http://schema.qcadoo.org/modules/windowTabExtension"
	xsi:schemaLocation="http://schema.qcadoo.org/modules/windowTabExtension http://schema.qcadoo.org/modules/windowTabExtension.xsd"
	plugin="basic" view="parameters">

	<windowTab name="materialFlowResourcesTab" reference="materialFlowResourcesTab">
		<component type="gridLayout" name="materialFlowResourcesGridLayout" columns="3" rows="1">
			<layoutElement column="1" row="1">
				<component type="select" name="changeDateWhenTransferToWarehouseType"
					field="#{form}.changeDateWhenTransferToWarehouseType" reference="changeDateWhenTransferToWarehouseType" 
					hasDescription="true" />
			</layoutElement>
		</component>
	</windowTab>
	
</windowTabExtension>